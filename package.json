{
  "name": "shellby",
  "productName": "Shellby",
  "version": "1.0.0",
  "description": "Modern SSH/SFTP terminal with AI assistant, file manager, and clipboard manager by FintonLabs",
  "main": "dist/main/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/justynroberts/shellby.git"
  },
  "homepage": "https://github.com/justynroberts/shellby",
  "bugs": {
    "url": "https://github.com/justynroberts/shellby/issues"
  },
  "scripts": {
    "start": "webpack serve --config webpack.renderer.config.js --mode development",
    "build": "npm run build:preload && npm run build:main && npm run build:renderer",
    "build:preload": "webpack --config webpack.preload.config.js --mode production",
    "build:main": "webpack --config webpack.main.config.js --mode production",
    "build:renderer": "webpack --config webpack.renderer.config.js --mode production",
    "dev": "npm run build && electron .",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tsc --noEmit",
    "package": "npm run build && electron-builder"
  },
  "keywords": [
    "ssh",
    "sftp",
    "terminal",
    "secure",
    "electron",
    "ai-assistant",
    "file-manager",
    "clipboard",
    "fintonlabs",
    "xterm"
  ],
  "author": "FintonLabs <justyn@fintonlabs.com>",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "electron": "^38.2.2",
    "electron-store": "^8.2.0",
    "lucide-react": "^0.545.0",
    "node-forge": "^1.3.1",
    "openai": "^6.3.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "ssh2": "^1.17.0",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-webpack": "^7.10.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.2",
    "@types/node-forge": "^1.3.14",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/ssh2": "^1.15.5",
    "@types/uuid": "^10.0.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^30.2.0",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
